PAPERLESS_AI_INITIAL_SETUP=yes
PAPERLESS_API_URL=http://localhost:8000/api
PAPERLESS_API_TOKEN=xxxxxxxxxxxxxxxxxxxx
PAPERLESS_USERNAME=clusterzx
AI_PROVIDER=custom
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5-nano
OLLAMA_API_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2
SCAN_INTERVAL=*/30 * * * *
MIN_CONTENT_LENGTH=10

# Token Limits - Auto-configured based on model if not specified
# TOKEN_LIMIT sets the maximum context window size
# Model-specific defaults (current models only):
#   - gpt-5, gpt-5-standard: 1,000,000 tokens (1M context)
#   - gpt-5-nano, gpt-5-mini: 200,000 tokens (200K context)
#   - o3-mini: 200,000 tokens
#   - gpt-4.5: 128,000 tokens
#   - gpt-4.1: 128,000 tokens
# TOKEN_LIMIT=200000

# RESPONSE_TOKENS sets max output tokens reserved for AI response
# Model-specific defaults (current models only):
#   - gpt-5, gpt-5-standard: 16,384 tokens (capped at 4,096 for efficiency)
#   - gpt-5-nano, gpt-5-mini: 8,192 tokens (capped at 4,096)
#   - o3-mini: 8,192 tokens (capped at 4,096)
#   - gpt-4.5, gpt-4.1: 8,192 tokens (capped at 4,096)
# RESPONSE_TOKENS=4096

# SYSTEM_PROMPT - The AI system prompt (DEPRECATED - now in system-prompt.md)
# The system prompt is now loaded from the system-prompt.md file in the root directory.
# You can edit system-prompt.md directly instead of setting this environment variable.
# If both exist, the file takes precedence. This env var is kept for backward compatibility.
# Leave blank to use the default prompt from system-prompt.md
SYSTEM_PROMPT=

PROCESS_PREDEFINED_DOCUMENTS=yes
TAGS=pre-process
ADD_AI_PROCESSED_TAG=no
AI_PROCESSED_TAG_NAME=ai-processed
USE_PROMPT_TAGS=no
PROMPT_TAGS=
USE_EXISTING_DATA=no
API_KEY=xxxxxxxxxxxxxxxxxxxxxxxxxxxxx
CUSTOM_API_KEY=xxxxxxxxxxxxxxxxxxxxxxx
CUSTOM_BASE_URL=https://api.deepseek.com/v1
CUSTOM_MODEL=deepseek-chat
